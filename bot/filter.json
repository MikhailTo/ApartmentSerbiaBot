{   
    "nekretnine":
    {
        "base": "https://www.nekretnine.rs",
        "path_style": "True",
        "city":
            {
                "base": "grad",
                "beograd": "beograd",
                "novi_sad": "novi-sad",
                "nis": "nis"
            },
        "type":
            {
                "apartaments":  "apartmani",
                "houses":       "stambeni-objekti"
            },
        "rooms":
            {
                "_comment": "no rooms"
            },
        "area": 
            {
                "base": "kvadratura",
                "from_to": "{from}_{to}"
            },
        "price": 
            {
                "base": "cena",
                "from_to": "{from}_{to}"
            },
        "pages": 
            {
                "base": "lista/po-stranici",
                "min": "10",
                "max": "20"
            }
    },
    "fourzida":
    {
        "base": "https://www.4zida.rs",
        "city":
            {
                "_comment": "path_style - the second",
                "beograd": "beograd",
                "novi_sad": "novi-sad",
                "nis": "nis"
            },
        "type":
            {
                "_comment": "path_style - the first",
                "apartaments":  "prodaja-stanova",
                "houses":       "prodaja-kuca"
            },
        "rooms":
            {
                "_comment": "path_style for 1",
                "base": "struktura=",
                "0.5": "garsonjera",
                "1": "jednosoban",
                "1.5": "jednoiposoban",
                "2": "dvosoban",
                "2.5": "dvoiposoban",
                "3": "trosoban",
                "3.5": "troiposoban",
                "4": "cetvorosoban",
                "4.5": "cetvoroiposoban-i-vise"
            },
        "area": 
            {
                "from": "vece_od={from}m2",
                "to": "manje_od={to}m2" 
            },
        "price": 
            {
                "from":"skuplje_od={from}eur",
                "_comment": "path_style for to",
                "to": "do-{to}-evra" 
            }
    },
    "cityexpert":
    {
        "base": "https://cityexpert.rs",
        "city":
            {
                "_comment": "path_style",
                "beograd": "prodaja-nekretnina/beograd",
                "novi_sad": "prodaja-nekretnina/novi-sad",
                "nis": "prodaja-nekretnina/nis"
            },
        "type":
            {   
                "_comment": "1,2",
                "base": "ptId=",
                "apartaments":  "1",
                "houses":       "2"
            },
        "rooms":
            {
                "_comment": "0.5,1.0",
                "base":"structure=",
                "0.5": "0.5",
                "1.0": "1.0",
                "1.5": "1.5",
                "2.0": "2.0",
                "2.5": "2.5",
                "3.0": "3.0",
                "3.5": "3.5",
                "4.0": "4.0",
                "4.5": "4.5"
            },
        "area": 
            {
                "from": "minSize={from}",
                "to": "maxSize={to}"
            },
        "price": 
            {
                "from": "minPrice={from}",
                "to": "maxPrice={to}"
            }
    }

}

